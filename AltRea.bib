	file = {arXiv Fulltext PDF:C\:\\Users\\huben\\Zotero\\storage\\AW6TDUPG\\Unzicker - 2008 - Why do we Still Believe in Newton's Law  Facts, M.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\huben\\Zotero\\storage\\LVVWHSPN\\0702009.html:text/html},
}

