@misc{unzicker_displacement_2003,
	title = {Displacement {Field} and {Elastic} {Energy} of a {Circular} {Twist} {Disclination} for {Large} {Deformations} - an {Example} how to {Treat} {Nonlinear} {Boundary} {Value} {Problems} with {Computer} {Algebra} {Systems}},
